# This is a generated file. Do not edit it.
# If you call this as a batch file, it converts all
# image files into png/jpg files for HTML use.

cat usersguide-html-image-gallery-raw.tex | tbrplent > usersguide-html-image-gallery.tex
echo "<root \
    xmlns=\"http://tbookdtd.sourceforge.net/dimension-file\">" \
    > usersguide-html-image-sizes.xml


echo
echo "---- processing image 1/4 (\"objecttree-h5\") ----"
# 
if test ! $1 || test $1 == "objecttree-h5"; then
latex -interaction=batchmode \
    "\batchmode\def\PictureNumber{1}\def\BeginHook{}\input usersguide-html-image-gallery"
dvips -P tbook -O 0pt,0pt -q* -R -o objecttree-h5-for-conv.ps usersguide-html-image-gallery
gs -dTextAlphaBits=4 -dGraphicsAlphaBits=4 \
     -r100 -q -dNOPAUSE -dBATCH -sOutputFile=objecttree-h5-web.pnm -sDEVICE=ppm objecttree-h5-for-conv.ps
ppmtojpeg objecttree-h5-web.pnm --comment="(c) 2003 Francesc Alted" \
    --progressive --grayscale > objecttree-h5-web.jpg
cat objecttree-h5-web.pnm | pnmfile > objecttree-h5-image-size.txt
rm -f objecttree-h5-web.pnm
rm -f objecttree-h5-for-conv.ps
echo -n; else echo "**** skipped"; fi
echo "<entry file=\"objecttree-h5\">" >> usersguide-html-image-sizes.xml
cat objecttree-h5-image-size.txt >> usersguide-html-image-sizes.xml
echo "</entry>" >> usersguide-html-image-sizes.xml

echo
echo "---- processing image 2/4 (\"objecttree\") ----"
# 
if test ! $1 || test $1 == "objecttree"; then
latex -interaction=batchmode \
    "\batchmode\def\PictureNumber{2}\def\BeginHook{}\input usersguide-html-image-gallery"
dvips -P tbook -O 0pt,0pt -q* -R -o objecttree-for-conv.ps usersguide-html-image-gallery
gs -dTextAlphaBits=4 -dGraphicsAlphaBits=4 \
     -r100 -q -dNOPAUSE -dBATCH -sOutputFile=objecttree-web.pnm -sDEVICE=pnm objecttree-for-conv.ps
pnmtopng -interlace -compression 9 -text objecttree-image-comment.txt \
    objecttree-web.pnm > objecttree-web.png
cat objecttree-web.pnm | pnmfile > objecttree-image-size.txt
rm -f objecttree-web.pnm
rm -f objecttree-for-conv.ps
echo -n; else echo "**** skipped"; fi
echo "<entry file=\"objecttree\">" >> usersguide-html-image-sizes.xml
cat objecttree-image-size.txt >> usersguide-html-image-sizes.xml
echo "</entry>" >> usersguide-html-image-sizes.xml

echo
echo "---- processing image 3/4 (\"tutorial-h5\") ----"
# 
if test ! $1 || test $1 == "tutorial-h5"; then
latex -interaction=batchmode \
    "\batchmode\def\PictureNumber{3}\def\BeginHook{}\input usersguide-html-image-gallery"
dvips -P tbook -O 0pt,0pt -q* -R -o tutorial-h5-for-conv.ps usersguide-html-image-gallery
gs -dTextAlphaBits=4 -dGraphicsAlphaBits=4 \
     -r100 -q -dNOPAUSE -dBATCH -sOutputFile=tutorial-h5-web.pnm -sDEVICE=ppm tutorial-h5-for-conv.ps
ppmtojpeg tutorial-h5-web.pnm --comment="(c) 2003 Francesc Alted" \
    --progressive --grayscale > tutorial-h5-web.jpg
cat tutorial-h5-web.pnm | pnmfile > tutorial-h5-image-size.txt
rm -f tutorial-h5-web.pnm
rm -f tutorial-h5-for-conv.ps
echo -n; else echo "**** skipped"; fi
echo "<entry file=\"tutorial-h5\">" >> usersguide-html-image-sizes.xml
cat tutorial-h5-image-size.txt >> usersguide-html-image-sizes.xml
echo "</entry>" >> usersguide-html-image-sizes.xml

echo
echo "---- processing image 4/4 (\"tutorial2-h5\") ----"
# 
if test ! $1 || test $1 == "tutorial2-h5"; then
latex -interaction=batchmode \
    "\batchmode\def\PictureNumber{4}\def\BeginHook{}\input usersguide-html-image-gallery"
dvips -P tbook -O 0pt,0pt -q* -R -o tutorial2-h5-for-conv.ps usersguide-html-image-gallery
gs -dTextAlphaBits=4 -dGraphicsAlphaBits=4 \
     -r100 -q -dNOPAUSE -dBATCH -sOutputFile=tutorial2-h5-web.pnm -sDEVICE=ppm tutorial2-h5-for-conv.ps
ppmtojpeg tutorial2-h5-web.pnm --comment="(c) 2003 Francesc Alted" \
    --progressive --grayscale > tutorial2-h5-web.jpg
cat tutorial2-h5-web.pnm | pnmfile > tutorial2-h5-image-size.txt
rm -f tutorial2-h5-web.pnm
rm -f tutorial2-h5-for-conv.ps
echo -n; else echo "**** skipped"; fi
echo "<entry file=\"tutorial2-h5\">" >> usersguide-html-image-sizes.xml
cat tutorial2-h5-image-size.txt >> usersguide-html-image-sizes.xml
echo "</entry>" >> usersguide-html-image-sizes.xml

echo "</root>" >> usersguide-html-image-sizes.xml
