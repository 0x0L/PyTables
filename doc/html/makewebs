echo "---- processing image 3/17 (\"objecttree\") ----"
# 
if test -z "$1" || test "XX$1" = "XXobjecttree" ; then
latex -interaction=batchmode \
    "\batchmode\def\PictureNumber{3}\def\BeginHook{}\input usersguide-image-gallery"
